# https://en.wikipedia.org/wiki/Semigroup

structure semigroup
  head type

  link set, like set, have type
  link operation, like binary-operation, have type

  # Closure under the operation
  assume set-closure
    head type, have type
    have set
    have operation

  # Associativity of the operation
  assume set-associativity
    head type, have type
    have set
    have operation
